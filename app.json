{
  "expo": {
    "name": "Better Living",
    "slug": "better-living",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "entryPoint": "./src/App.tsx",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.patrickhanna.betterliving"
    },
    "android": {
      "useNextNotificationsApi": true
    }
  }
}
